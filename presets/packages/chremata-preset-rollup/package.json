{
  "name": "@chremata-preset/rollup",
  "version": "0.0.0",
  "description": "Chremata Rollup configuration",
  "license": "MIT",
  "engines": {
    "node": ">=24 <25"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lemuel-manske/chremata.git",
    "directory": "presets/packages/chremata-preset-rollup"
  },
  "private": true,
  "author": "lemuel-manske",
  "type": "module",
  "main": "./rollup-config.js",
  "types": "./rollup-config.d.ts",
  "exports": {
    ".": {
      "import": "./rollup-config.js",
      "types": "./rollup-config.d.ts"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "lint": "npm run lint:source:fix && npm run lint:format:fix",
    "lint:source": "eslint .",
    "lint:source:fix": "npm run lint:source -- --fix",
    "lint:format": "prettier --check --ignore-path .gitignore .",
    "lint:format:fix": "prettier --write --ignore-path .gitignore ."
  },
  "dependencies": {
    "@chremata-preset/swc": "file:../swc",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-swc": "^0.4.0",
    "@rollup/plugin-typescript": "^12.1.4",
    "@rollup/plugin-url": "^8.0.2",
    "postcss-import": "^16.1.1",
    "postcss-url": "^10.1.3",
    "rollup-plugin-postcss": "^4.0.2",
    "tslib": "^2.8.1"
  },
  "peerDependencies": {
    "rollup": "^4.49.0"
  },
  "devDependencies": {
    "@chremata-preset/eslint": "file:../chremata-preset-eslint",
    "@chremata-preset/prettier": "file:../chremata-preset-prettier",
    "@chremata-preset/typescript": "file:../chremata-preset-prettier",
    "eslint": "^9.35.0",
    "prettier": "^3.6.2",
    "typescript": "^5.9.2"
  }
}
